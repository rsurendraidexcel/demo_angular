  "description": "cync application",
  "scripts": {
    "ng": "ng",
    "version": "ng --version",
    "start": "ng serve --poll 500",
    "dev2": "ng serve --host dev2 --poll 500 --live-reload",
    "dev": "ng serve --host devrorapi --poll 500 --live-reload",
    "staging": "ng serve --host staging --poll 500 --live-reload",
    "build": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build --prod --build-optimizer=true",
    "test": "ng test",
    "test:coverage": "ng test --codeCoverage",
    "test:headless": "ng test --codeCoverage --browsers ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "source-map-build": "node ./node_modules/@angular/cli/bin/ng build --dev --sourcemaps",
    "lite": "lite-server -d ./dist",
    "source-map": "node ./node_modules/.bin/source-map-explorer dist/main.bundle.js",
    "lint-report": "node ./node_modules/.bin/gulp"
  },

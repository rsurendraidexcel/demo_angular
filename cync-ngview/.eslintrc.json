{  
    "extends": "eslint:recommended",
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"]
    },
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "globals": {
        "$": true,
        "require": true,
        "process": true
      },
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "amd": true
    }
}